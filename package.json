{
  "name": "react-in-isolation",
  "license": "MIT",
  "author": "Matt Travi",
  "repository": "travi/react-in-isolation",
  "bugs": "https://github.com/travi/react-in-isolation/issues",
  "homepage": "https://github.com/travi/react-in-isolation#readme",
  "scripts": {
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "lint:js": "eslint . .storybook --cache",
    "tests:unit": "mocha --recursive test/unit",
    "test": "run-s lint:* coverage",
    "coverage": "nyc run-s tests:unit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "predeploy": "run-s build-storybook",
    "deploy": "netlify deploy --site=$NETLIFY_SITE_ID --auth=$NETLIFY_ACCESS_TOKEN --dir=./storybook-static/ --prod"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/register": "7.12.1",
    "@storybook/addon-actions": "6.0.28",
    "@storybook/addon-info": "5.3.21",
    "@storybook/addon-links": "6.0.28",
    "@storybook/react": "6.0.28",
    "@travi/any": "2.0.13",
    "@travi/babel-preset": "3.0.33",
    "@travi/eslint-config": "1.0.54",
    "@travi/eslint-config-mocha": "1.0.8",
    "@travi/eslint-config-react": "1.0.51",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-preset-react": "6.24.1",
    "chai": "4.2.0",
    "commitlint-config-travi": "1.3.11",
    "cz-conventional-changelog": "3.3.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "globstar": "1.0.0",
    "husky": "4.3.0",
    "markdownlint-cli": "0.24.0",
    "mocha": "8.2.1",
    "netlify-cli": "2.68.0",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "react-test-renderer": "17.0.1",
    "sinon": "9.2.1",
    "storybook-addon-react-docgen": "1.2.42",
    "validate-commit-msg": "2.14.0"
  },
  "dependencies": {
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router": "5.2.0",
    "storybook-router": "0.3.4"
  }
}
