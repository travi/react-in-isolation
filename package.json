{
  "name": "react-in-isolation",
  "license": "MIT",
  "author": "Matt Travi",
  "repository": "travi/react-in-isolation",
  "bugs": "https://github.com/travi/react-in-isolation/issues",
  "homepage": "https://github.com/travi/react-in-isolation#readme",
  "scripts": {
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "lint:js": "eslint . .storybook --cache",
    "tests:unit": "mocha --recursive test/unit",
    "test": "run-s lint:* coverage",
    "coverage": "nyc run-s tests:unit",
    "commitmsg": "commitlint -e",
    "precommit": "npm test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "predeploy": "run-s build-storybook",
    "deploy": "netlify deploy --site=$NETLIFY_SITE_ID --auth=$NETLIFY_ACCESS_TOKEN --dir=./storybook-static/ --prod"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/register": "^7.0.0",
    "@storybook/addon-actions": "4.1.11",
    "@storybook/addon-info": "4.1.11",
    "@storybook/addon-links": "4.1.11",
    "@storybook/react": "4.1.11",
    "@travi/any": "1.9.0",
    "@travi/babel-preset": "3.0.0",
    "@travi/eslint-config-travi": "1.8.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-preset-react": "6.24.1",
    "chai": "^4.2.0",
    "commitlint-config-travi": "^1.2.31",
    "cz-conventional-changelog": "2.1.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "globstar": "1.0.0",
    "husky": "2.1.0",
    "markdownlint-cli": "^0.15.0",
    "mocha": "6.1.4",
    "netlify-cli": "2.11.15",
    "npm-run-all": "4.1.5",
    "nyc": "^14.0.0",
    "react-test-renderer": "16.7.0",
    "sinon": "7.3.2",
    "storybook-addon-react-docgen": "1.0.4",
    "validate-commit-msg": "2.14.0"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-router": "3.2.1",
    "storybook-router": "0.3.4"
  }
}
