{
  "name": "react-in-isolation",
  "license": "MIT",
  "author": "Matt Travi",
  "repository": "travi/react-in-isolation",
  "bugs": "https://github.com/travi/react-in-isolation/issues",
  "homepage": "https://github.com/travi/react-in-isolation#readme",
  "scripts": {
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "lint:js": "eslint . .storybook --cache",
    "tests:unit": "mocha --recursive test/unit",
    "test": "run-s lint:* coverage",
    "coverage": "nyc run-s tests:unit",
    "commitmsg": "commitlint -e",
    "precommit": "npm test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "predeploy": "run-s build-storybook",
    "deploy": "netlify deploy --site=$NETLIFY_SITE_ID --auth=$NETLIFY_ACCESS_TOKEN --dir=./storybook-static/ --prod"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/register": "^7.5.5",
    "@storybook/addon-actions": "5.2.1",
    "@storybook/addon-info": "5.2.1",
    "@storybook/addon-links": "5.2.1",
    "@storybook/react": "5.2.1",
    "@travi/any": "2.0.6",
    "@travi/babel-preset": "3.0.2",
    "@travi/eslint-config-travi": "1.8.4",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-preset-react": "6.24.1",
    "chai": "^4.2.0",
    "commitlint-config-travi": "^1.3.3",
    "cz-conventional-changelog": "3.0.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "globstar": "1.0.0",
    "husky": "^3.0.3",
    "markdownlint-cli": "^0.18.0",
    "mocha": "6.2.1",
    "netlify-cli": "^2.12.0",
    "npm-run-all": "4.1.5",
    "nyc": "^14.1.1",
    "react-test-renderer": "^16.9.0",
    "sinon": "^7.4.1",
    "storybook-addon-react-docgen": "^1.2.5",
    "validate-commit-msg": "2.14.0"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router": "^5.0.1",
    "storybook-router": "0.3.4"
  }
}
