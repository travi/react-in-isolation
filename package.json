{
  "name": "react-in-isolation",
  "license": "MIT",
  "author": "Matt Travi",
  "repository": "travi/react-in-isolation",
  "bugs": "https://github.com/travi/react-in-isolation/issues",
  "homepage": "https://github.com/travi/react-in-isolation#readme",
  "scripts": {
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "lint:js": "eslint . .storybook --cache",
    "tests:unit": "mocha --recursive test/unit",
    "test": "run-s lint:* coverage",
    "coverage": "nyc run-s tests:unit",
    "commitmsg": "commitlint -e",
    "precommit": "npm test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "predeploy": "run-s build-storybook",
    "deploy": "netlify deploy --site=$NETLIFY_SITE_ID --auth=$NETLIFY_ACCESS_TOKEN --dir=./storybook-static/ --prod"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.3.10",
    "@storybook/addon-links": "3.4.6",
    "@storybook/react": "3.3.10",
    "@travi/any": "1.8.2",
    "@travi/eslint-config-travi": "1.5.21",
    "babel-eslint": "8.2.3",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "commitlint-config-travi": "1.2.20",
    "cz-conventional-changelog": "2.1.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "globstar": "1.0.0",
    "husky": "^1.1.3",
    "markdownlint-cli": "0.9.0",
    "mocha": "5.2.0",
    "netlify-cli": "^2.1.3",
    "npm-run-all": "4.1.2",
    "nyc": "^13.1.0",
    "react-test-renderer": "16.4.0",
    "sinon": "4.4.6",
    "validate-commit-msg": "2.14.0"
  },
  "dependencies": {
    "prop-types": "15.6.1",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "react-router": "3.2.0",
    "storybook-router": "0.3.4"
  }
}
