{
  "name": "react-in-isolation",
  "license": "MIT",
  "author": "Matt Travi",
  "repository": "travi/react-in-isolation",
  "bugs": "https://github.com/travi/react-in-isolation/issues",
  "homepage": "https://github.com/travi/react-in-isolation#readme",
  "scripts": {
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "lint:js": "eslint . .storybook --cache",
    "tests:unit": "mocha --recursive test/unit",
    "test": "run-s lint:* coverage",
    "coverage": "nyc run-s tests:unit",
    "commitmsg": "commitlint -e",
    "precommit": "npm test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy": "gh-pages -r \"https://$GH_TOKEN@github.com/travi/react-in-isolation.git\" -d storybook-static/",
    "greenkeeper:update-lockfile": "greenkeeper-lockfile-update",
    "greenkeeper:upload-lockfile": "greenkeeper-lockfile-upload"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.2.18",
    "@storybook/addon-links": "3.2.18",
    "@storybook/react": "3.2.14",
    "@travi/any": "1.6.0",
    "@travi/eslint-config-travi": "1.5.21",
    "babel-eslint": "8.0.3",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "commitlint-config-travi": "1.2.19",
    "cz-conventional-changelog": "2.1.0",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.0",
    "gh-pages": "1.1.0",
    "globstar": "1.0.0",
    "greenkeeper-lockfile": "1.12.0",
    "husky": "0.14.3",
    "markdownlint-cli": "0.3.1",
    "mocha": "4.0.1",
    "npm-run-all": "4.1.2",
    "nyc": "11.3.0",
    "react-test-renderer": "16.2.0",
    "sinon": "4.1.3",
    "validate-commit-msg": "2.14.0"
  },
  "dependencies": {
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-router": "3.2.0",
    "storybook-router": "0.3.2"
  }
}
