    "enzyme-adapter-react-16": "1.0.1",
    "globstar": "1.0.0",
    "husky": "0.14.3",
    "markdownlint-cli": "0.3.1",
    "mocha": "4.0.0",
    "npm-run-all": "4.1.1",
    "nyc": "11.2.1",
    "react-test-renderer": "16.0.0",
