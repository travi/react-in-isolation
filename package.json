{
  "name": "react-in-isolation",
  "license": "MIT",
  "private": true,
  "author": "Matt Travi",
  "repository": "travi/react-in-isolation",
  "bugs": "https://github.com/travi/react-in-isolation/issues",
  "homepage": "https://github.com/travi/react-in-isolation#readme",
  "scripts": {
    "lint:md": "remark . --frail",
    "lint:js": "eslint . .storybook --cache",
    "tests:unit": "mocha --recursive test/unit",
    "test": "run-s lint:* coverage",
    "coverage": "nyc run-s tests:unit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "generate:md": "remark . --output",
    "predeploy:netlify": "run-s build-storybook",
    "deploy:netlify": "netlify deploy --site=$NETLIFY_SITE_ID --auth=$NETLIFY_ACCESS_TOKEN --dir=./storybook-static/ --prod",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/register": "7.17.7",
    "@storybook/addon-actions": "6.4.19",
    "@storybook/addon-info": "5.3.21",
    "@storybook/addon-links": "6.4.19",
    "@storybook/react": "6.4.19",
    "@travi/any": "2.0.20",
    "@travi/babel-preset": "3.0.82",
    "@travi/eslint-config": "1.0.94",
    "@travi/eslint-config-mocha": "1.0.16",
    "@travi/eslint-config-react": "1.0.79",
    "babel-loader": "8.2.4",
    "babel-preset-react": "6.24.1",
    "chai": "4.3.6",
    "commitlint-config-travi": "1.4.10",
    "cz-conventional-changelog": "3.3.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "globstar": "1.0.0",
    "husky": "8.0.3",
    "mocha": "9.2.2",
    "netlify-cli": "8.1.4",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "react-test-renderer": "17.0.2",
    "remark-cli": "10.0.1",
    "remark-preset-lint-travi": "2.0.9",
    "remark-toc": "8.0.1",
    "sinon": "13.0.1",
    "storybook-addon-react-docgen": "1.2.42",
    "validate-commit-msg": "2.14.0"
  },
  "dependencies": {
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router": "6.3.0",
    "storybook-router": "0.3.4"
  }
}
